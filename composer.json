{
    "name": "webignition/stream-factory-interface",
    "description": "Interface for a factory to create Psr\\Http\\Message\\StreamInterface objects",
    "keywords": ["stream", "psr-7"],
    "homepage": "https://github.com/webignition/stream-factory-interfaces",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\StreamFactoryInterface\\": "src/"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src --colors --standard=PSR2"
    },
    "require": {
        "php": ">=7.2.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3"
    },
    "minimum-stability":"stable"
}